	hu = input(f'{M} ╰─  {P}Pilih urutan id : ')
	if hu in ['1','01']:
		for tua in sorted(id):
			id2.append(tua)

	elif hu in ['2','02']:
		muda=[]
		for bacot in sorted(id):
			muda.append(bacot)
		bcm=len(muda)
		bcmi=(bcm-1)
		for xmud in range(bcm):
			id2.append(muda[bcmi])
			bcmi -=1
	elif hu in ['3','03']:
		for bacot in id:
			xx = random.randint(0,len(id2))
			id2.insert(xx,bacot)
	else:
		print(' ╰─  Pilih Yang Bener Kontooll ')
		exit()
	tree = Tree('')
	tree.add(f'{h}1. {P}MOBILE')
	tree.add(f'{h}2. {P}MBASIC')
	tree.add(f'{h}3. {P}FREE')
	tree.add(f'{h}4. {P}MOBILE V2')
	cetak(tree)
